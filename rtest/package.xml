<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>rtest</name>
  <version>0.2.0</version>
  <description>ROS2-specific code to help with testing.</description>
  <maintainer email="gc19@beam.global">Gary Cross</maintainer>
  <maintainer email="slawomir.cielepak@gmail.com">SÅ‚awomir Cielepak</maintainer>

  <license>Apache 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_ros</buildtool_depend>
  <depend>rclcpp</depend>
  <depend>rclcpp_action</depend>
  <depend>gmock_vendor</depend>
  <depend>action_msgs</depend>
  <depend>rcl</depend>
  <depend>rcl_action</depend>

  <test_depend>ament_cmake_copyright</test_depend>
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_clang_tidy</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
    <rosdoc2>rosdoc2.yaml</rosdoc2>
    <quality_level_status content="LEVEL_4"/>
  </export>
</package>
